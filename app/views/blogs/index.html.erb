<div class="container">
    <%= link_to "Create a New Blog", new_blog_path, class:"btn" %>
</div>

<div class="container-box">
    <h1>All Blogs</h1>
</div>

<div class="container-box-blog">
    <% @blogs.each do |blog| %>
        <p><%= blog.title %></p>
        <p><%= blog.body %></p>
    <% end %>
</div>

